dup8 {

  Clone 8 bits from A to B
  ########,
  :#/#/#/#/#/#/#/#,
  #######,
  :#/#/#/#/#/#/#,
  ######,
  :#/#/#/#/#/#,
  #####,
  :#/#/#/#/#,
  ####,
  :#/#/#/#,
  ###,
  :#/#/#,
  ##,
  :#/#,
  #,
  :#
  
  Transfer reverse from B to A
  ##,/,
  #,/#/,#
  #,/#/#/,##
  #,/#/#/#/,###
  #,/#/#/#/#/,####
  #,/#/#/#/#/#/,#####
  #,/#/#/#/#/#/#/,######
  ,

}
